		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-storybook": "^0.6.12",
		"eslint-plugin-svelte": "^2.26.0",
		"jest": "^29.5.0",
		"jest-axe": "^7.0.1",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.24",
		"postcss-import": "^15.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.0.20",
